                    <th style="padding-left: 80px; padding-right: 80px">AKsi</th>
                  </tr>
                </thead>
                <tbody>
                    <% for(let i = 0; i < customers.length; i++){ %>
                    <tr key={user._id}>
                      <td style="text-align: center"><%= i +1 %></td>
                      <td style="padding-left: 10px"><%= customers[i].name %></td>
                      <td style="padding-left: 10px"> <%= customers[i].email %></td>
					  <td></td>
					  <!-- <td style="padding-left: 10px;padding-bottom:20px;"> <%= customers[i].password %></td> -->
                      <td class="text-center">
                        <a href="/updateCustomer/<%= customers[i]._id %>" class="btn btn-outline-warning btn-sm button-update">
						Edit
					    </a>

                        <a href="/deleteCustomer/<%= customers[i]._id %>" class="btn btn-outline-danger btn-sm button-delete">
							deleted
						</a>
                      </td>

                    </tr>
                    <% } %>
