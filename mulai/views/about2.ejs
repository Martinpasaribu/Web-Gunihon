	<section class="service">
		<div class="container">
        <h1 class="judul" style="text-align:center" > About </h1>
        <p class="penjelas"> Ubah Tempat </p>
        
		<form action="/users/aboutedit" method="POST">
            <p>
                Jalan 
            </p>
            <input type="teks" id="teks" name="teks" class="form-input"  value="<%= typeof teks != 'undefined' ? teks : '' %>" placeholder="" autocomplete="off" required>
               
            
            <p>
                Kota 
            </p>
            <input type="name" id="name" name="name" class="form-input"  value="<%= typeof name != 'undefined' ? name : '' %>" placeholder="" autocomplete="off" required>
            
			  <button type="submit" class="btn btn-primary btn-block">
				Kirim
			  </button>
        </form>

     
        <br><br><br>
        <h3 class="judul" style=" text-align: center;" >Contacts Us</h3>

        <div className="columns mt-5">
            <div className="column is-half">
              <Link to="add" className="button is-success">
              =======
              </Link>
              <table border="1" style=" margin-right: 60px;margin-left: 60px;">
                <thead>
                  <tr bgcolor="#9d97a3" align="center"bgcolor="#9d97a3" align="center">
                    
                    <th colspan="1" style="padding-left: 20px; padding-right: 20px">No</th>
                    <th style="padding-left: 70px; padding-right: 70px">Jalan</th>
                    <th style="padding-left: 70px; padding-right: 70px">Kota</th>
                    <th style="padding-left: 80px; padding-right: 80px">-</th>
                    <th style="padding-left: 80px; padding-right: 80px">Aksi</th>
                  </tr>
                </thead>
                <tbody>
                    <% for(let i = 0; i < abouts.length; i++){ %>
                    <tr key={user._id}>
                      <td style="text-align: center"><%= i +1 %></td>
                      <td style="padding-left: 10px"><%= abouts[i].teks %></td>
                      <td style="padding-left: 10px"> <%= abouts[i].name %></td>
                 
                      <td class="text-center">


                        <td class="text-center">


				   
                          <a href="/updateabout/<%= abouts[i]._id %>" class="btn btn-outline-warning btn-sm button-update">
                            Edit
                            </a>
                
                            <a href="/deleteAbout/<%= abouts[i]._id %>" class="btn btn-outline-danger btn-sm button-delete">
                              deleted
                            </a>
                
                
                          </td>

                    </tr>
                    <% } %>
                </tbody>
              </table>
            </div>
          </div>

    </div>
</section>







