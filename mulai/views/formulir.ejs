	<!-- baris -->
	<!-- <div class="medsos">
		<div class="container">
			
			
				<atur >Home / Produk / Formulir </atur>
		</br>
			



		</div>
	</div> -->

	<section class="service">
		<div class="container">
        <h1 class="judul" style="text-align:center" > Formulir </h1>
        <p class="penjelas"> Masukan Data Diri Anda Dengan Lengkap dan Benar </p>
        
		<form action="/users/formulir" method="POST">
            <p>
                Nama 
            </p>
            <input type="name" id="name" name="name" class="form-input"  value="<%= typeof name != 'undefined' ? name : '' %>" placeholder="" autocomplete="off" required>
            
			<p>
                Jenis Kelamin 
			 </p>
				<select name="jk" id="jk" name="jk" class="form-input">
				 <option ></option>
				 <option value="Laki-Laki"> Laki-Laki</option>
				 <option value="Perempuan"> Perempuan</option>

			 
			   </select>

            <p>
                No WhatsApp
            </p>
            <input type="nowa" id="nowa" name="nowa" class="form-input"   value="<%= typeof nowa != 'undefined' ? nowa : '' %>" placeholder=" " autocomplete="off" required>
            <p>
                Email
            </p>
            <input type="email" id="email" name="email" class="form-input" placeholder="" autocomplete="off" required value="<%= typeof email != 'undefined' ? email : '' %>">

            <p>
               Status
            </p>
			   <select name="sts" id="sts" name="sts" class="form-input">
                <option></option>
                <option value="Pelajar"> Pelajar</option>
                <option value="Pekerja"> Pekerja</option>
                <option value="Pendidik"> Pendidik</option>
            
              </select>
            
            <p>
                Kelas Program yang dipilih 
            </p>
         
            <select name="program" id="program" name="program" class="form-input"">
                <option ></option>
                <option value="Beginer Level">Beginer Level </option>
                <option value="Basic Level"> Basic Level</option>
                <option value="Intermediate Level"> Intermediate Level</option>
                <option value="Advanced Level"> Advanced Level </option>
				<option value="Speaking Class"> Speaking Class </option>
				<option value="Intensive Japanese"> Intensive Japanese  </option>
                
              </select>
			
            
			  <button type="submit" class="btn btn-primary btn-block">
				Kirim
			  </button>
        </form>


    </div>
</section>







	<!-- footer -->
	<footer>
		<div class="container">
			<small>Copyright &copy; 2022 - Go-Project. All Rights Reserved.</small>
		</div>
	</footer>


	<script type="text/javascript">
		$(document).ready(function(){
			$(".bg-loader").hide();
		})

		
	</script>
 
   
</body>
</html>