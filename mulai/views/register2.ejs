<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3"><i class="fas fa-user-plus"></i> Daftar Sebagai Admin</h1>
        <%= include ("./partials/messages"); %>
        
        <form action="/users/reglogin2" method="POST">
          <div class="form-group">
            <label for="name">Nama Lengkap</label>
            <input
              type="name"
              id="name"
              name="name"
              class="form-control"
              placeholder="Enter Name"
              value="<%= typeof name != 'undefined' ? name : '' %>"
            />
          </div>

          <div class="form-group">
            <label for="email">Email</label>
            <input
              type="email"
              id="email"
              name="email"
              class="form-control"
              placeholder="Enter Email"
              value="<%= typeof email != 'undefined' ? email : '' %>"
            />
          </div>
          <div class="form-group">
            <label for="password">Password</label>
            <input
              type="password"
              id="password"
              name="password"
              class="form-control"
              placeholder="Create Password"
              value="<%= typeof password != 'undefined' ? password : '' %>"
            />
          </div>
         
          <button type="submit" class="btn btn-primary btn-block">
           Daftar
          </button>
        </form>
         <a style="text-align:center ; padding-top:20px;" href="/users/login2" style="color:rgb(6, 123, 43)" > <p1 >Masuk</p1></a>
      </div>
    </div>
  </div>
  